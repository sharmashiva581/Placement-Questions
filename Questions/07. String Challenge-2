import java.util.*;
import java.io.*;
public class Main
{
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		String input = scn.nextLine();
		String ti = scn.nextLine();
		String ans = stringChallenge(input , ti);
		for(int i=0 ; i < ans.length() ; i++){
		    System.out.print(ans.charAt(i) );
		}
	}
	public static String stringChallenge(String str , String ti){
	    
	    String res = "";
	    for(int i=0 ; i+1 < str.length(); i++){
	        int v1 = str.charAt(i)-'0';
	        int v2 = str.charAt(i+1)-'0';
	        
	        if(v1 % 2==0 && v2 % 2==0){ // both num are even case
	            res = res + v1 + '*';
	           
	        }
	        else if(v1 % 2 !=0 && v2 % 2 !=0){ // odd numbers case
	            res = res + v1 + '-' ;
	           
	        }
	        else{
	            res = res + v1;
	           
	        }
	        if(i+1 == str.length()-1){
	            res += str.charAt(i+1);
	        }
	    }
	    StringBuilder sb = new StringBuilder(res);
	    for(int i=0 ; i < sb.length(); i++){
	        char ch = sb.charAt(i);
	        boolean ans = check(ch , ti);
	        if(ans == true){
	            sb.deleteCharAt(i);
	        }
	    }
	    return sb.toString();
	}
	public static boolean check(char ch , String ti){
	    int idx =  ti.indexOf(ch);
	    if(idx == -1){
	        return false;
	    }
	    else {
	        return true;
	    }
	}
}
